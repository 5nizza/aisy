// realizable
module bench(clk, stay, controllable_reset, err);
  input clk;
  input stay;
  input controllable_reset;
  output err;
  reg [8:0] counter;

  assign err = (counter == 9'b111111111) ? 1 : 0;
  
  initial
  begin
    counter = 9'b0;
  end
  always @ (posedge clk)
  begin
    if(stay)
      counter = counter;
    else if(counter == 9'b011111111 && controllable_reset)
      counter = 0;
    else
      counter = counter + 1;
   end
endmodule
