// realizable
module bench(clk, a, controllable_c, err);
  input clk;
  input [7:0] a;
  input [7:0] controllable_c;
  reg [8:0] counter;
  output err;
  wire ctrl_xor;
  
  assign err = (counter == 9'b111111111) ? 1 : 0;
  assign ctrl_xor = controllable_c[0] ^ 
                    controllable_c[1] ^
                    controllable_c[2] ^
                    controllable_c[3] ^
                    controllable_c[4] ^
                    controllable_c[5] ^
                    controllable_c[6] ^
                    controllable_c[6];

  initial
  begin
    counter = 0;
  end

  always @ (posedge clk)
  begin
    if(counter[8] & ctrl_xor)
       counter[8] = 0;
    else
       counter = counter + {1'b0, a};
   end
endmodule
