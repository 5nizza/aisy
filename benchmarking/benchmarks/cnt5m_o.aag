// realizable
module bench(clk, stay, controllable_reset, err);
  input clk;
  input stay;
  input controllable_reset;
  output err;
  reg [4:0] counter;

  assign err = (counter == 5'b11111) ? 1 : 0;
  
  initial
  begin
    counter = 5'b0;
  end
  always @ (posedge clk)
  begin
    if(stay)
      counter = counter;
    else if(counter == 5'b01111 && controllable_reset)
      counter = 0;
    else
      counter = counter + 1;
   end
endmodule
