// realizable
module bench(clk, a, b, controllable_c, err);
  input clk;
  input [7:0] a;
  input [7:0] b;
  input [7:0] controllable_c;
  output err;
  reg err;

  initial
  begin
    err = 1'b0;
  end

  always @ (posedge clk)
  begin
    if(controllable_c == a + b)
         err = 1'b0;
      else
         err = 1'b1;
   end
endmodule
